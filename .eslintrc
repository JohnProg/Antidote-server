{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "strict": 0,
    "max-len": [2, 120],
    "no-param-reassign": 0
  },
  "env": {
    "node": true
  },
  "globals": {
    "describe": true,
    "it": true
  }
}